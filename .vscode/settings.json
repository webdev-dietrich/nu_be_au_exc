{
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.rules.customizations": [
    {
      "rule": "@stylistic/*",
      "fixable": true,
      "severity": "info"
    }
  ],
  "eslint.validate": ["javascript", "typescript", "vue", "html", "markdown", "json", "json5", "jsonc", "css", "vue"],
}